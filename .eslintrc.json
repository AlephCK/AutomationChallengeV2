{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": [
    "playwright"
  ],
  "extends": [
    "plugin:playwright/playwright-test"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}